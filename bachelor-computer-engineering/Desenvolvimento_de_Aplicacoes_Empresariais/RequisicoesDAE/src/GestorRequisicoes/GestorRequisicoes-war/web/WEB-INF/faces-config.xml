    </managed-bean>
    <navigation-rule>
        <from-view-id>/secureAprovador/RequisicoesExistentes.jsp</from-view-id>
        <navigation-case>
            <from-outcome>VER</from-outcome>
            <to-view-id>/DetalhesRequisicao.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>secureAprovador$RequisicoesPendentes</managed-bean-name>
        <managed-bean-class>gestorrequisicoeswar.secureAprovador.RequisicoesPendentes</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>NewReqEmpStep2</managed-bean-name>
        <managed-bean-class>gestorrequisicoeswar.NewReqEmpStep2</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/index.jsp</from-view-id>
        <navigation-case>
            <from-outcome>newReqStep2</from-outcome>
            <to-view-id>/NewReqEmpStep2.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secureAprovador/RequisicoesPendentes.jsp</from-view-id>
        <navigation-case>
            <from-outcome>VER</from-outcome>
            <to-view-id>/DetalhesRequisicao.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ALTERAR</from-outcome>
            <to-view-id>/AlterarRequisicao.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/MinhasRequisicoes.jsp</from-view-id>
        <navigation-case>
            <from-outcome>VER</from-outcome>
            <to-view-id>/DetalhesRequisicao.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>AlterarRequisicao</managed-bean-name>
        <managed-bean-class>gestorrequisicoeswar.AlterarRequisicao</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/AlterarRequisicao.jsp</from-view-id>
        <navigation-case>
            <from-outcome>GUARDAR</from-outcome>
            <to-view-id>/secureAprovador/RequisicoesPendentes.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>CANCELAR</from-outcome>
            <to-view-id>/secureAprovador/RequisicoesPendentes.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>secureResponsavel$RequisicoesExistentes</managed-bean-name>
        <managed-bean-class>gestorrequisicoeswar.secureResponsavel.RequisicoesExistentes</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/secureResponsavel/RequisicoesExistentes.jsp</from-view-id>
        <navigation-case>
            <from-outcome>VER</from-outcome>
            <to-view-id>/DetalhesRequisicao.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Rascunhos.jsp</from-view-id>
        <navigation-case>
            <from-outcome>VER</from-outcome>
            <to-view-id>/DetalhesRequisicao.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
